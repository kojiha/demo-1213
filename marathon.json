{
  "apps": [
    {
      "id": "/demo/demoapp",
      "instances": 1,
      "cpus": 0.1,
      "mem": 1024,
      "disk": 0,
      "gpus": 0,
      "constraints": [],
      "fetch": [],
      "storeUrls": [],
      "backoffSeconds": 1,
      "backoffFactor": 1.15,
      "maxLaunchDelaySeconds": 3600,
      "container": {
        "type": "DOCKER",
        "volumes": [],
        "docker": {
          "image": "registry.marathon.l4lb.thisdcos.directory:5000/demo/demoapp:latest",
          "network": "BRIDGE",
          "portMappings": [
            {
              "containerPort": 8080,
              "hostPort": 0,
              "protocol": "tcp"
            }
          ],
          "privileged": false,
          "parameters": [],
          "forcePullImage": true
        }
      },
      "healthChecks": [],
      "readinessChecks": [],
      "dependencies": ["/demo/demodb"],
      "upgradeStrategy": {
        "minimumHealthCapacity": 1,
        "maximumOverCapacity": 1
      },
      "unreachableStrategy": {
        "inactiveAfterSeconds": 300,
        "expungeAfterSeconds": 600
      },
      "killSelection": "YOUNGEST_FIRST",
      "requirePorts": false,
      "env": {
        "JAVA_OPTS": "-Xmx512m -Xms512m"
      },
      "labels": {
        "HAPROXY_GROUP": "external",
        "HAPROXY_0_VHOST": "demoapp.app.dcos.local"
      }
    },
    {
      "id": "/demo/demodb",
      "backoffFactor": 1.15,
      "backoffSeconds": 1,
      "container": {
        "portMappings": [
          {
            "containerPort": 5432,
            "servicePort": 0
          }
        ],
        "type": "DOCKER",
        "volumes": [
          {
            "containerPath": "pgdata",
            "mode": "RW",
            "persistent": {
              "size": 100
            }
          }
        ],
        "docker": {
          "image": "registry.marathon.l4lb.thisdcos.directory:5000/demo/demodb:latest",
          "forcePullImage": false,
          "privileged": false,
          "parameters": []
        }
      },
      "cpus": 0.1,
      "disk": 0,
      "env": {
        "POSTGRES_USER": "postgres",
        "POSTGRES_PASSWORD": "postgres",
        "PGDATA": "/mnt/mesos/sandbox/pgdata"
      },
      "instances": 1,
      "maxLaunchDelaySeconds": 3600,
      "mem": 512,
      "gpus": 0,
      "networks": [
        {
          "name": "dcos",
          "mode": "container"
        }
      ],
      "requirePorts": false,
      "residency": {
        "taskLostBehavior": "WAIT_FOREVER"
      },
      "upgradeStrategy": {
        "maximumOverCapacity": 0,
        "minimumHealthCapacity": 0
      },
      "killSelection": "YOUNGEST_FIRST",
      "unreachableStrategy": {
        "inactiveAfterSeconds": 0,
        "expungeAfterSeconds": 0
      },
      "healthChecks": [],
      "fetch": [],
      "constraints": []
    }
  ]  
}
